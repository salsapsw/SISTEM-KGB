{
    "version": 2,
    "builds": [
        {
            "src": "public/**/*",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(.*).(css|js|jpg|png|svg)",
            "dest": "public/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "public/index.php"
        }
    ]
}
